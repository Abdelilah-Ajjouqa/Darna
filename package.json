{
  "name": "darna-api",
  "version": "0.0.1",
  "description": "Darna is an API for real Estate managments and selling",
  "main": "index.js",
  "scripts": {
    "build:src": "tsc -p tsconfig.src.json",
    "build:sso": "tsc -p tsconfig.sso.json",
    "build": "npm run build:src && npm run build:sso",
    "start": "node out/app.js",
    "start:sso": "node SSO-out/app.js",
    "dev": "nodemon --watch src --exec ts-node src/app.ts",
    "dev:sso": "nodemon --watch SSO --exec ts-node SSO/index.ts",
    "clean": "rimraf out SSO-out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OussamaBenoujja/Darna.git"
  },
  "keywords": [
    "RealEstate",
    "API",
    "MongoDB",
    "TypeScript"
  ],
  "author": "Oussama Benoujja - Abdillah Ajouqa - Anouar El Aissaoui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OussamaBenoujja/Darna/issues"
  },
  "homepage": "https://github.com/OussamaBenoujja/Darna#readme",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/mongoose": "^5.11.97",
    "@types/node": "^24.9.0",
    "typescript": "^5.9.3"
  }
}
